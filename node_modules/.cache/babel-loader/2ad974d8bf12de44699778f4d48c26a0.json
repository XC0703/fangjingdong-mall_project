{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { get } from '../../utils/request';\nimport ShopInfo from '../../components/ShopInfo';\nimport ContentView from './ContentView';\nimport CartFooter from './CartFooter.vue';\n\nconst useShopInsoEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n\n  const getItemData = async () => {\n    const result = await get(`api/shop/${route.params.id}`);\n\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data;\n    }\n  };\n\n  const {\n    item\n  } = toRefs(data);\n  return {\n    item,\n    getItemData\n  };\n};\n\nconst useBackRouterEffect = () => {\n  const router = useRouter();\n\n  const handleBackClick = () => {\n    router.back();\n  };\n\n  return {\n    handleBackClick\n  };\n};\n\nexport default {\n  name: 'ShopView',\n  components: {\n    ShopInfo,\n    ContentView,\n    CartFooter\n  },\n\n  setup() {\n    const {\n      item,\n      getItemData\n    } = useShopInsoEffect();\n    const {\n      handleBackClick\n    } = useBackRouterEffect();\n    getItemData();\n    return {\n      item,\n      handleBackClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0BA,SAASA,QAAT,EAAmBC,MAAnB,QAAiC,KAAjC;AACA,SAASC,SAAT,EAAmBC,QAAnB,QAAmC,YAAnC;AACA,SAASC,GAAT,QAAoB,qBAApB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,WAAP,MAAwB,eAAxB;AACA,OAAOC,UAAP,MAAuB,kBAAvB;;AAEA,MAAMC,iBAAgB,GAAI,MAAI;EAC1B,MAAMC,KAAI,GAAIN,QAAQ,EAAtB;EACA,MAAMO,IAAG,GAAIV,QAAQ,CAAC;IACtBW,IAAI,EAAC;EADiB,CAAD,CAArB;;EAGA,MAAMC,WAAU,GAAI,YAAS;IACzB,MAAMC,MAAK,GAAI,MAAMT,GAAG,CAAE,YAAWK,KAAK,CAACK,MAAN,CAAaC,EAAG,EAA7B,CAAxB;;IACA,IAAGF,MAAM,EAAEG,KAAR,KAAkB,CAAlB,IAAqBH,MAAM,EAAEH,IAAhC,EAAqC;MACjCA,IAAI,CAACC,IAAL,GAAYE,MAAM,CAACH,IAAnB;IACJ;EACJ,CALA;;EAMA,MAAM;IAAEC;EAAF,IAAWV,MAAM,CAACS,IAAD,CAAvB;EACA,OAAO;IAAEC,IAAF;IAAOC;EAAP,CAAP;AACJ,CAbA;;AAcA,MAAMK,mBAAkB,GAAI,MAAI;EAC5B,MAAMC,MAAK,GAAIhB,SAAS,EAAxB;;EACA