{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { get } from '../../utils/request';\nimport ShopInfo from '../../components/ShopInfo';\nimport Content from './ContentView';\nimport Cart from './CartFooter'; // 获取当前商铺信息\n\nconst useShopInfoEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n\n  const getItemData = async () => {\n    const result = await get(`/api/shop/${route.params.id}`);\n\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data;\n    }\n  };\n\n  const {\n    item\n  } = toRefs(data);\n  return {\n    item,\n    getItemData\n  };\n}; // 点击回退逻辑\n\n\nconst useBackRouterEffect = () => {\n  const router = useRouter();\n\n  const handleBackClick = () => {\n    router.back();\n  };\n\n  return handleBackClick;\n};\n\nexport default {\n  name: 'ShopView',\n  components: {\n    ShopInfo,\n    Content,\n    Cart\n  },\n\n  setup() {\n    const {\n      item,\n      getItemData\n    } = useShopInfoEffect();\n    const handleBackClick = useBackRouterEffect();\n    getItemData();\n    return {\n      item,\n      handleBackClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,SAASA,QAAT,EAAmBC,MAAnB,QAAiC,KAAjC;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AACA,SAASC,GAAT,QAAoB,qBAApB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,IAAP,MAAiB,cAAjB,C,CAEA;;AACA,MAAMC,iBAAgB,GAAI,MAAM;EAC9B,MAAMC,KAAI,GAAIN,QAAQ,EAAtB;EACA,MAAMO,IAAG,GAAIV,QAAQ,CAAC;IAAEW,IAAI,EAAE;EAAR,CAAD,CAArB;;EACA,MAAMC,WAAU,GAAI,YAAY;IAC9B,MAAMC,MAAK,GAAI,MAAMT,GAAG,CAAE,aAAYK,KAAK,CAACK,MAAN,CAAaC,EAAG,EAA9B,CAAxB;;IACA,IAAGF,MAAM,EAAEG,KAAR,KAAkB,CAAlB,IAAuBH,MAAM,EAAEH,IAAlC,EAAwC;MACtCA,IAAI,CAACC,IAAL,GAAYE,MAAM,CAACH,IAAnB;IACF;EACF,CALA;;EAMA,MAAM;IAAEC;EAAF,IAAWV,MAAM,CAACS,IAAD,CAAvB;EACA,OAAO;IAAEC,IAAF;IAAQC;EAAR,CAAP;AACF,CAXA,C,CAaA;;;AACA,MAAMK,mBAAkB,GAAI,MAAM;EAChC,MAAMC,MAAK,GAAIhB,SAAS,EAAxB;;EACA,MAAMiB,eAAc,GAAI,MAAM;IAC5BD,MAAM,CAACE,IAAP;EACF,CAFA;;EAGA,OAAOD,eAAP;AACF,CANA;;AAQA,eAAe;EACbE,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IAAEjB,QAAF;IAAYC,OAAZ;IAAqBC;EAArB,CAFC;;EAGbgB,KAAK,GAAG;IACN,MAAM;MAAEZ,IAAF;MAAQC;IAAR,IAAwBJ,iBAAiB,EAA/C;IACA,MAAMW,eAAc,GAAIF,mBAAmB,EAA3C;IACAL,WAAW;IACX,OAAO;MAAED,IAAF;MAAQQ;IAAR,CAAP;EACF;;AARa,CAAf","names":["reactive","toRefs","useRouter","useRoute","get","ShopInfo","Content","Cart","useShopInfoEffect","route","data","item","getItemData","result","params","id","errno","useBackRouterEffect","router","handleBackClick","back","name","components","setup"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\shop\\ShopView.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"search\">\r\n      <div class=\"search__back iconfont\" @click=\"handleBackClick\">\r\n        &#xe6f2;\r\n      </div>\r\n      <div class=\"search__content\">\r\n        <span class=\"search__content__icon iconfont\">&#xe62d;</span>\r\n        <input\r\n          class=\"search__content__input\"\r\n          placeholder=\"请输入商品名称\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <ShopInfo :item=\"item\" :hideBorder=\"true\" v-show=\"item.imgUrl\"/>\r\n    <Content :shopName=\"item.name\" />\r\n    <Cart />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { get } from '../../utils/request'\r\nimport ShopInfo from '../../components/ShopInfo'\r\nimport Content from './ContentView'\r\nimport Cart from './CartFooter'\r\n\r\n// 获取当前商铺信息\r\nconst useShopInfoEffect = () => {\r\n  const route = useRoute()\r\n  const data = reactive({ item: {} })\r\n  const getItemData = async () => {\r\n    const result = await get(`/api/shop/${route.params.id}`)\r\n    if(result?.errno === 0 && result?.data) {\r\n      data.item = result.data\r\n    }\r\n  }\r\n  const { item } = toRefs(data)\r\n  return { item, getItemData }\r\n}\r\n\r\n// 点击回退逻辑\r\nconst useBackRouterEffect = () => {\r\n  const router = useRouter()\r\n  const handleBackClick = () => {\r\n    router.back()\r\n  }\r\n  return handleBackClick\r\n}\r\n\r\nexport default {\r\n  name: 'ShopView',\r\n  components: { ShopInfo, Content, Cart },\r\n  setup() {\r\n    const { item, getItemData } = useShopInfoEffect()\r\n    const handleBackClick = useBackRouterEffect()\r\n    getItemData()\r\n    return { item, handleBackClick }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n  padding: 0 .18rem;\r\n}\r\n.search {\r\n  display: flex;\r\n  margin: .14rem 0 .04rem 0;\r\n  line-height: .32rem;\r\n  &__back {\r\n    width: .3rem;\r\n    font-size: .24rem;\r\n    color: #B6B6B6;\r\n  }\r\n  &__content {\r\n    display: flex;\r\n    flex: 1;\r\n    background: $search-bgColor;\r\n    border-radius: .16rem;\r\n    &__icon {\r\n      width: .44rem;\r\n      text-align: center;\r\n      color: $search-fontColor;\r\n    }\r\n    &__input {\r\n      display: block;\r\n      width: 100%;\r\n      padding-right: .2rem;\r\n      border: none;\r\n      outline: none;\r\n      background: none;\r\n      height: .32rem;\r\n      font-size: .14rem;\r\n      color: $content-fontcolor;\r\n      &::placeholder {\r\n        color: $content-fontcolor;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}