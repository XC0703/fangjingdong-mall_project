{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport { get } from '../../utils/request';\nimport { useRoute } from 'vue-router'; // 点击获取tab的逻辑\n\nconst useTabEffect = categories => {\n  let currentTab = categories[0].tab;\n\n  const handleTabClick = item => {\n    currentTab = item.tab;\n  };\n\n  return {\n    currentTab,\n    handleTabClick\n  };\n}; // 根据tab获取对应数据的逻辑\n\n\nconst getContentEffect = currentTab => {\n  const route = useRoute();\n  const shopId = route.params.id;\n  const contentList = reactive({\n    contentList: []\n  });\n\n  const getContentData = async () => {\n    const result = await get(`/api/shop/${shopId}/products`, {\n      tab: currentTab.value\n    });\n\n    if (result.data.errno === 0) {\n      contentList.value = result.data.data;\n    }\n  }; // getContentData('all')\n\n\n  watchEffect(() => {\n    getContentData(currentTab);\n  });\n  return {\n    route,\n    contentList,\n    getContentData,\n    shopId\n  };\n};\n\nexport default {\n  name: 'ContentView',\n\n  setup() {\n    const categories = [{\n      name: '全部商品',\n      tab: 'all'\n    }, {\n      name: '秒杀',\n      tab: 'seckill'\n    }, {\n      name: '新鲜水果',\n      tab: 'fruit'\n    }];\n    const data = reactive({\n      currentTab: categories[0].tab,\n      contentList: []\n    });\n\n    const getContentData = async tab => {\n      const result = await get('api/shop/1/products', {\n        tab\n      });\n\n      if (result?.errno === 0 && result?.data?.length) {\n        data.contentList = result.data;\n      }\n    };\n\n    const handleCategoryClick = tab => {\n      getContentData(tab);\n      data.currentTab = tab;\n    };\n\n    getContentData('all');\n    const {\n      contentList,\n      currentTab\n    } = toRefs(data);\n    return {\n      contentList,\n      categories,\n      currentTab,\n      handleCategoryClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,SAASA,QAAT,EAAmBC,MAAnB,QAAkC,KAAlC;AACA,SAASC,GAAT,QAAoB,qBAApB;AACA,SAAQC,QAAR,QAAwB,YAAxB,C,CAEA;;AACA,MAAMC,YAAW,GAAKC,UAAD,IAAc;EACjC,IAAIC,UAAS,GAAID,UAAU,CAAC,CAAD,CAAV,CAAcE,GAA/B;;EACA,MAAMC,cAAa,GAAKC,IAAD,IAAQ;IAC/BH,UAAS,GAAIG,IAAI,CAACF,GAAlB;EACA,CAFA;;EAIA,OAAO;IAACD,UAAD;IAAYE;EAAZ,CAAP;AACF,CAPA,C,CAQA;;;AACA,MAAME,gBAAe,GAAKJ,UAAD,IAAgB;EACvC,MAAMK,KAAI,GAAIR,QAAQ,EAAtB;EACA,MAAMS,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;EACA,MAAMC,WAAU,GAAIf,QAAQ,CAAC;IACzBe,WAAW,EAAC;EADa,CAAD,CAA5B;;EAGA,MAAMC,cAAa,GAAI,YAAY;IACjC,MAAMC,MAAK,GAAI,MAAMf,GAAG,CAAE,aAAYU,MAAO,WAArB,EAAiC;MAAEL,GAAG,EAAED,UAAU,CAACY;IAAlB,CAAjC,CAAxB;;IACA,IAAID,MAAM,CAACE,IAAP,CAAYC,KAAZ,KAAsB,CAA1B,EAA6B;MAC3BL,WAAW,CAACG,KAAZ,GAAoBD,MAAM,CAACE,IAAP,CAAYA,IAAhC;IACF;EACF,CALA,CANuC,CAYvC;;;EACAE,WAAW,CAAC,MAAM;IAChBL,cAAc,CAACV,UAAD,CAAd;EACD,CAFU,CAAX;EAGA,OAAO;IAAEK,KAAF;IAASI,WAAT;IAAsBC,cAAtB;IAAsCJ;EAAtC,CAAP;AACF,CAjBA;;AAkBA,eAAe;EACbU,IAAI,EAAE,aADO;;EAEbC,KAAK,GAAG;IACN,MAAMlB,UAAS,GAAI,CAAC;MAClBiB,IAAI,EAAC,MADa;MAElBf,GAAG,EAAC;IAFc,CAAD,EAGjB;MACAe,IAAI,EAAC,IADL;MAEAf,GAAG,EAAC;IAFJ,CAHiB,EAMjB;MACAe,IAAI,EAAC,MADL;MAEAf,GAAG,EAAC;IAFJ,CANiB,CAAnB;IAUA,MAAMY,IAAG,GAAInB,QAAQ,CAAC;MACpBM,UAAU,EAACD,UAAU,CAAC,CAAD,CAAV,CAAcE,GADL;MAEpBQ,WAAW,EAAC;IAFQ,CAAD,CAArB;;IAIA,MAAMC,cAAa,GAAI,MAAMT,GAAN,IAAY;MACjC,MAAMU,MAAK,GAAI,MAAMf,GAAG,CAAC,qBAAD,EAAuB;QAC7CK;MAD6C,CAAvB,CAAxB;;MAGA,IAAGU,MAAM,EAAEG,KAAR,KAAgB,CAAhB,IAAmBH,MAAM,EAAEE,IAAR,EAAcK,MAApC,EAA2C;QACzCL,IAAI,CAACJ,WAAL,GAAmBE,MAAM,CAACE,IAA1B;MACF;IACF,CAPA;;IAQA,MAAMM,mBAAmB,GAAElB,GAAD,IAAO;MAC/BS,cAAc,CAACT,GAAD,CAAd;MACAY,IAAI,CAACb,UAAL,GAAkBC,GAAlB;IACF,CAHA;;IAIAS,cAAc,CAAC,KAAD,CAAd;IACA,MAAM;MAAED,WAAF;MAAcT;IAAd,IAA6BL,MAAM,CAACkB,IAAD,CAAzC;IACA,OAAO;MAACJ,WAAD;MAAaV,UAAb;MAAwBC,UAAxB;MAAmCmB;IAAnC,CAAP;EACF;;AAhCa,CAAf","names":["reactive","toRefs","get","useRoute","useTabEffect","categories","currentTab","tab","handleTabClick","item","getContentEffect","route","shopId","params","id","contentList","getContentData","result","value","data","errno","watchEffect","name","setup","length","handleCategoryClick"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\shop\\ContentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"category\">\r\n        <div \r\n          class=\"category__item\" \r\n          :class=\"{'category__item--active': currentTab===item.tab}\"\r\n          v-for=\"item in categories\"\r\n          :key=\"item.name\"\r\n          @click=\"()=>handleCategoryClick(item.tab)\"\r\n        >{{item.name}}</div>\r\n    </div>\r\n    <div class=\"product\">\r\n        <div \r\n          class=\"product__item\"\r\n          v-for=\"item in contentList\"\r\n          :key=\"item._id\"\r\n        >\r\n            <img class=\"product__item__img\" src=\"http://www.dell-lee.com/imgs/vue3/near.png\" />\r\n            <div class=\"product__item__detail\">\r\n              <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n              <p class=\"product__item__sales\">月售 {{item.sales}} 件</p>\r\n              <p class=\"product__item__price\">\r\n                <span class=\"product__item__yen\">&yen;</span>{{item.price}}\r\n                <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n              </p>\r\n            </div>\r\n            <div class=\"product__number\">\r\n              <span class=\"product__number__minus\">-</span>\r\n              0\r\n              <span class=\"product__number__plus\">+</span>\r\n            </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs, } from 'vue';\r\nimport { get } from '../../utils/request';\r\nimport {useRoute } from 'vue-router'\r\n\r\n// 点击获取tab的逻辑\r\nconst useTabEffect = (categories)=>{\r\n  let currentTab = categories[0].tab;\r\n  const handleTabClick = (item)=>{\r\n  currentTab = item.tab;\r\n  }\r\n\r\n  return {currentTab,handleTabClick}\r\n}\r\n// 根据tab获取对应数据的逻辑\r\nconst getContentEffect = (currentTab) => {\r\n  const route = useRoute()\r\n  const shopId = route.params.id\r\n  const contentList = reactive({\r\n      contentList:[]\r\n  });\r\n  const getContentData = async () => {\r\n    const result = await get(`/api/shop/${shopId}/products`, { tab: currentTab.value })\r\n    if (result.data.errno === 0) {\r\n      contentList.value = result.data.data\r\n    }\r\n  }\r\n  // getContentData('all')\r\n  watchEffect(() => {\r\n    getContentData(currentTab)\r\n  })\r\n  return { route, contentList, getContentData, shopId }\r\n}\r\nexport default {\r\n  name: 'ContentView',\r\n  setup() {\r\n    const categories = [{\r\n      name:'全部商品',\r\n      tab:'all'\r\n    },{\r\n      name:'秒杀',\r\n      tab:'seckill'\r\n    },{\r\n      name:'新鲜水果',\r\n      tab:'fruit'\r\n    }]\r\n    const data = reactive({\r\n      currentTab:categories[0].tab,\r\n      contentList:[]\r\n    });\r\n    const getContentData = async(tab)=>{\r\n      const result = await get('api/shop/1/products',{\r\n        tab\r\n      })\r\n      if(result?.errno===0&&result?.data?.length){\r\n        data.contentList = result.data\r\n      }\r\n    }\r\n    const handleCategoryClick=(tab)=>{\r\n      getContentData(tab);\r\n      data.currentTab = tab;\r\n    }\r\n    getContentData('all');\r\n    const { contentList,currentTab } = toRefs(data)\r\n    return {contentList,categories,currentTab,handleCategoryClick}\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n.content {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 1.5rem;\r\n  bottom: .5rem;\r\n}\r\n.category {\r\n  overflow-y: scroll;\r\n  height: 100%;\r\n  width: .76rem;\r\n  background: $search-bgColor;\r\n  &__item {\r\n    line-height: .4rem;\r\n    text-align: center;\r\n    font-size: 14px;\r\n    color: $content-fontcolor;\r\n    &--active {\r\n      background: $bgColor;\r\n    }\r\n  }\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    margin: 0 .16rem;\r\n    border-bottom: .01rem solid $content-bgcolor;\r\n    &__detail{\r\n      overflow: hidden;\r\n    }\r\n    &__img {\r\n      width: .68rem;\r\n      height: .68rem;\r\n      margin-right: .16rem;\r\n    }\r\n    &__title{\r\n      margin:0;\r\n      line-height:.2rem;\r\n      font-size: .14rem;\r\n      color:$content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__sales{\r\n      margin:.06rem 0;\r\n      line-height: .16rem;\r\n      font-size: .12rem;\r\n      color:$content-fontcolor;\r\n    }\r\n    &__price{\r\n      margin:0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color:$hightlight-fontColor ;\r\n    }\r\n    &__yen{\r\n      font-size: .12rem;\r\n    }\r\n    &__origin{\r\n      margin-left: .06rem;\r\n      line-height: .2rem;\r\n      font-size: .12rem;\r\n      color:$light-fontColor;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number{\r\n      position: absolute;\r\n      right:0;\r\n      bottom:.12rem;\r\n      &__minus,&__plus{\r\n        display: inline-block;\r\n        width: .2rem;\r\n        height: .2rem;\r\n        line-height: .16rem;\r\n        border-radius: 50%;\r\n        font-size: .2rem;     \r\n        text-align: center;\r\n      }\r\n      &__minus{\r\n        border: .01rem solid $medium-fontColor;\r\n        color: $medium-fontColor;\r\n        margin-right: .05rem;\r\n      }\r\n      &__plus{\r\n        margin-left: .05rem;\r\n        background-color: #0091ff;\r\n        color: $bgColor;\r\n      }\r\n    }\r\n  }\r\n  \r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}