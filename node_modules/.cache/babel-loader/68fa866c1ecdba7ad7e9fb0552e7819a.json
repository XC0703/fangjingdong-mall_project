{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router'; // 获取购物车信息逻辑\n\nconst useCartEffect = () => {\n  const store = useStore();\n  const route = useRoute();\n  const shopId = route.params.id;\n  const cartList = store.state.cartList;\n  const total = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n\n    if (productList) {\n      for (let i in productList) {\n        const product = productList[i];\n        count += product.count;\n      }\n    }\n\n    return count;\n  });\n  const price = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n\n    if (productList) {\n      for (let i in productList) {\n        const product = productList[i];\n        count += product.count * product.price;\n      }\n    }\n\n    return count.toFixed(2);\n  });\n  return {\n    total,\n    price\n  };\n};\n\nexport default {\n  name: 'CartFooter',\n\n  setup() {\n    const {\n      total,\n      price\n    } = useCartEffect();\n    return {\n      total,\n      price\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB,C,CAEA;;AACA,MAAMC,aAAY,GAAI,MAAM;EACxB,MAAMC,KAAI,GAAIH,QAAQ,EAAtB;EACA,MAAMI,KAAI,GAAIH,QAAQ,EAAtB;EACA,MAAMI,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;EACA,MAAMC,QAAO,GAAIL,KAAK,CAACM,KAAN,CAAYD,QAA7B;EAEA,MAAME,KAAI,GAAIX,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAD,CAA5B;IACA,IAAIO,KAAI,GAAI,CAAZ;;IACA,IAAGD,WAAH,EAAgB;MACb,KAAI,IAAIE,CAAR,IAAaF,WAAb,EAA0B;QACxB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAD,CAA3B;QACAD,KAAI,IAAKE,OAAO,CAACF,KAAjB;MACH;IACF;;IACA,OAAOA,KAAP;EACD,CAVqB,CAAtB;EAYA,MAAMG,KAAI,GAAIhB,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAD,CAA5B;IACA,IAAIO,KAAI,GAAI,CAAZ;;IACA,IAAGD,WAAH,EAAgB;MACb,KAAI,IAAIE,CAAR,IAAaF,WAAb,EAA0B;QACxB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAD,CAA3B;QACAD,KAAI,IAAME,OAAO,CAACF,KAAR,GAAgBE,OAAO,CAACC,KAAlC;MACH;IACF;;IACA,OAAOH,KAAK,CAACI,OAAN,CAAc,CAAd,CAAP;EACD,CAVqB,CAAtB;EAWA,OAAO;IAAEN,KAAF;IAASK;EAAT,CAAP;AACJ,CA9BA;;AA+BA,eAAe;EACbE,IAAI,EAAE,YADO;;EAEbC,KAAK,GAAG;IACN,MAAM;MAAER,KAAF;MAASK;IAAT,IAAmBb,aAAa,EAAtC;IACA,OAAO;MAAEQ,KAAF;MAASK;IAAT,CAAP;EACF;;AALa,CAAf","names":["computed","useStore","useRoute","useCartEffect","store","route","shopId","params","id","cartList","state","total","productList","count","i","product","price","toFixed","name","setup"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\shop\\CartFooter.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <div class=\"check\">\r\n      <div class=\"check__icon\">\r\n        <img\r\n          src=\"http://www.dell-lee.com/imgs/vue3/basket.png\"\r\n          class=\"check__icon__img\"\r\n        />\r\n        <div class=\"check__icon__tag\">{{total}}</div>\r\n      </div>\r\n      <div class=\"check__info\">\r\n        总计：<span class=\"check__info__price\">&yen; {{price}}</span>\r\n      </div>\r\n      <div class=\"check__btn\">去结算</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRoute } from 'vue-router'\r\n\r\n// 获取购物车信息逻辑\r\nconst useCartEffect = () => {\r\n    const store = useStore()\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const cartList = store.state.cartList\r\n\r\n    const total = computed(() => {\r\n      const productList = cartList[shopId]\r\n      let count = 0\r\n      if(productList) {\r\n         for(let i in productList) {\r\n           const product = productList[i]\r\n           count += product.count\r\n        }\r\n      }\r\n      return count\r\n    })\r\n\r\n    const price = computed(() => {\r\n      const productList = cartList[shopId]\r\n      let count = 0\r\n      if(productList) {\r\n         for(let i in productList) {\r\n           const product = productList[i]\r\n           count += (product.count * product.price)\r\n        }\r\n      }\r\n      return count.toFixed(2)\r\n    })\r\n    return { total, price}\r\n}\r\nexport default {\r\n  name: 'CartFooter',\r\n  setup() {\r\n    const { total, price } = useCartEffect()\r\n    return { total, price }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.cart {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n.check {\r\n  display: flex;\r\n  height: .49rem;\r\n  border-top: .01rem solid #f5f5f5;\r\n  line-height: .49rem;\r\n  &__icon {\r\n    position: relative;\r\n    width: .84rem;\r\n    &__img {\r\n      display: block;\r\n      margin: .12rem auto;\r\n      width: .28rem;\r\n      height: .26rem;\r\n    }\r\n    &__tag {\r\n      position: absolute;\r\n      left: .46rem;\r\n      top: .04rem;\r\n      padding: 0 .04rem;\r\n      min-width: .2rem;\r\n      height: .2rem;\r\n      line-height: .2rem;\r\n      background-color: $hightlight-fontColor;\r\n      border-radius: .1rem;\r\n      font-size: .12rem;\r\n      text-align: center;\r\n      color: #fff;\r\n      transform: scale(.5);\r\n      transform-origin: left center;\r\n    }\r\n  }\r\n  &__info {\r\n    flex: 1;\r\n    color: $content-fontcolor;\r\n    font-size: .12rem;\r\n    &__price {\r\n      line-height: .49rem;\r\n      color: $hightlight-fontColor;\r\n      font-size: .18rem;\r\n    }\r\n  }\r\n  &__btn {\r\n    width: .98rem;\r\n    background-color: #4FB0F9;\r\n    text-align: center;\r\n    color: #FFF;\r\n    font-size: .14rem;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}