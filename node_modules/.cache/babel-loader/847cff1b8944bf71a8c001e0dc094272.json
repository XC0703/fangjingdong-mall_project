{"ast":null,"code":"import { toRefs, reactive } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter, useRoute } from 'vue-router';\nimport { post, get, patch } from '../../utils/request';\nimport useCommonAddressEffect from '../../effects/addressEffect';\n\nconst useAddressEffect = () => {\n  const store = useStore();\n  const {\n    addressList\n  } = toRefs(store.state);\n  const {\n    getAddressList\n  } = useCommonAddressEffect();\n  getAddressList();\n  return {\n    addressList\n  };\n};\n\nconst useFormEffect = addressId => {\n  const router = useRouter();\n  const formData = reactive({\n    city: '',\n    department: '',\n    houseNumber: '',\n    name: '',\n    phone: ''\n  });\n\n  const getAddressItem = async () => {\n    const result = await get(`/api/user/address/${addressId}`);\n\n    if (result?.errno === 0 && result?.data) {\n      const {\n        city,\n        department,\n        houseNumber,\n        name,\n        phone\n      } = result.data;\n      formData.city = city;\n      formData.department = department;\n      formData.houseNumber = houseNumber;\n      formData.name = name;\n      formData.phone = phone;\n    }\n  };\n\n  const upsertAddress = async () => {\n    if (addressId) {\n      const result = await patch(`/api/user/address/${addressId}`, {\n        data: formData\n      });\n\n      if (result?.errno === 0) {\n        router.back();\n      }\n    } else {\n      const result = await post('/api/user/address', {\n        data: formData\n      });\n\n      if (result?.errno === 0) {\n        router.back();\n      }\n    }\n  };\n\n  if (addressId) {\n    getAddressItem();\n  }\n\n  return {\n    formData,\n    upsertAddress\n  };\n};\n\nexport default {\n  name: 'MyAddressList',\n\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const addressId = route?.params?.id;\n    const {\n      addressList\n    } = useAddressEffect();\n    const {\n      formData,\n      upsertAddress\n    } = useFormEffect(addressId);\n\n    const handleBackClick = () => {\n      router.back();\n    };\n\n    return {\n      addressId,\n      addressList,\n      formData,\n      handleBackClick,\n      upsertAddress\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,SAASA,MAAT,EAAiBC,QAAjB,QAAiC,KAAjC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AACA,SAASC,IAAT,EAAeC,GAAf,EAAoBC,KAApB,QAAiC,qBAAjC;AACA,OAAOC,sBAAP,MAAmC,6BAAnC;;AAEA,MAAMC,gBAAe,GAAI,MAAM;EAC7B,MAAMC,KAAI,GAAIR,QAAQ,EAAtB;EACA,MAAM;IAAES;EAAF,IAAkBX,MAAM,CAACU,KAAK,CAACE,KAAP,CAA9B;EACA,MAAM;IAAEC;EAAF,IAAqBL,sBAAsB,EAAjD;EACAK,cAAc;EACd,OAAO;IAAEF;EAAF,CAAP;AACF,CANA;;AAQA,MAAMG,aAAY,GAAKC,SAAD,IAAe;EACnC,MAAMC,MAAK,GAAIb,SAAS,EAAxB;EACA,MAAMc,QAAO,GAAIhB,QAAQ,CAAC;IACxBiB,IAAI,EAAE,EADkB;IACdC,UAAU,EAAE,EADE;IACEC,WAAW,EAAE,EADf;IACmBC,IAAI,EAAE,EADzB;IAC6BC,KAAK,EAAE;EADpC,CAAD,CAAzB;;EAGA,MAAMC,cAAa,GAAI,YAAY;IACjC,MAAMC,MAAK,GAAI,MAAMlB,GAAG,CAAE,qBAAoBS,SAAU,EAAhC,CAAxB;;IACA,IAAGS,MAAM,EAAEC,KAAR,KAAkB,CAAlB,IAAuBD,MAAM,EAAEE,IAAlC,EAAwC;MACtC,MAAM;QAAER,IAAF;QAAQC,UAAR;QAAoBC,WAApB;QAAiCC,IAAjC;QAAuCC;MAAvC,IAAiDE,MAAM,CAACE,IAA9D;MACAT,QAAQ,CAACC,IAAT,GAAgBA,IAAhB;MACAD,QAAQ,CAACE,UAAT,GAAsBA,UAAtB;MACAF,QAAQ,CAACG,WAAT,GAAuBA,WAAvB;MACAH,QAAQ,CAACI,IAAT,GAAgBA,IAAhB;MACAJ,QAAQ,CAACK,KAAT,GAAiBA,KAAjB;IACF;EACF,CAVA;;EAWA,MAAMK,aAAY,GAAI,YAAY;IAChC,IAAGZ,SAAH,EAAc;MACZ,MAAMS,MAAK,GAAI,MAAMjB,KAAK,CAAE,qBAAoBQ,SAAU,EAAhC,EAAmC;QAC3DW,IAAI,EAAET;MADqD,CAAnC,CAA1B;;MAGA,IAAIO,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;QAAET,MAAM,CAACY,IAAP;MAAc;IAC1C,CALD,MAKM;MACJ,MAAMJ,MAAK,GAAI,MAAMnB,IAAI,CAAC,mBAAD,EAAsB;QAC7CqB,IAAI,EAAET;MADuC,CAAtB,CAAzB;;MAGA,IAAIO,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;QAAET,MAAM,CAACY,IAAP;MAAc;IAC3C;EACF,CAZA;;EAaA,IAAGb,SAAH,EAAc;IAAEQ,cAAc;EAAG;;EACjC,OAAO;IAAEN,QAAF;IAAYU;EAAZ,CAAP;AACF,CA/BA;;AAiCA,eAAe;EACbN,IAAI,EAAE,eADO;;EAEbQ,KAAK,GAAG;IACN,MAAMb,MAAK,GAAIb,SAAS,EAAxB;IACA,MAAM2B,KAAI,GAAI1B,QAAQ,EAAtB;IACA,MAAMW,SAAQ,GAAIe,KAAK,EAAEC,MAAP,EAAeC,EAAjC;IACA,MAAM;MAAErB;IAAF,IAAkBF,gBAAgB,EAAxC;IACA,MAAM;MAAEQ,QAAF;MAAYU;IAAZ,IAA8Bb,aAAa,CAACC,SAAD,CAAjD;;IACA,MAAMkB,eAAc,GAAI,MAAM;MAAEjB,MAAM,CAACY,IAAP;IAAc,CAA9C;;IACA,OAAO;MAAEb,SAAF;MAAaJ,WAAb;MAA0BM,QAA1B;MAAoCgB,eAApC;MAAqDN;IAArD,CAAP;EACF;;AAVa,CAAf","names":["toRefs","reactive","useStore","useRouter","useRoute","post","get","patch","useCommonAddressEffect","useAddressEffect","store","addressList","state","getAddressList","useFormEffect","addressId","router","formData","city","department","houseNumber","name","phone","getAddressItem","result","errno","data","upsertAddress","back","setup","route","params","id","handleBackClick"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\addressUpdate\\AddressUpdate.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"title\">\r\n      <div\r\n        class=\"iconfont title__back\"\r\n        @click=\"handleBackClick\"\r\n      >&#xe6f2;</div>\r\n      <div class=\"title__text\">\r\n        {{addressId ? '编辑收货地址': '新建收货地址'}}\r\n      </div>\r\n      <div class=\"title__save\" @click=\"upsertAddress\">保存</div>\r\n    </div>\r\n    <div class=\"form\">\r\n      <div class=\"form__item\">\r\n        <div class=\"form__item__label\">所在城市:</div>\r\n        <input class=\"form__item__content\" placeholder=\"如北京市\" v-model=\"formData.city\" />\r\n      </div>\r\n      <div class=\"form__item\">\r\n        <div class=\"form__item__label\">小区/大厦/学校:</div>\r\n        <input class=\"form__item__content\" placeholder=\"如理工大学国防科技园\" v-model=\"formData.department\" />\r\n      </div>\r\n      <div class=\"form__item\">\r\n        <div class=\"form__item__label\">楼号-门牌号:</div>\r\n        <input class=\"form__item__content\" placeholder=\"A号楼B层\" v-model=\"formData.houseNumber\" />\r\n      </div>\r\n      <div class=\"form__item\">\r\n        <div class=\"form__item__label\">收货人:</div>\r\n        <input class=\"form__item__content\" placeholder=\"请填写收货人的姓名\" v-model=\"formData.name\" />\r\n      </div>\r\n      <div class=\"form__item\">\r\n        <div class=\"form__item__label\">联系电话:</div>\r\n        <input class=\"form__item__content\" placeholder=\"请填写收货手机号\" v-model=\"formData.phone\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { toRefs, reactive } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { post, get, patch } from '../../utils/request'\r\nimport useCommonAddressEffect from '../../effects/addressEffect'\r\n\r\nconst useAddressEffect = () => {\r\n  const store = useStore()\r\n  const { addressList } = toRefs(store.state)\r\n  const { getAddressList } = useCommonAddressEffect()\r\n  getAddressList()\r\n  return { addressList }\r\n}\r\n\r\nconst useFormEffect = (addressId) => {\r\n  const router = useRouter()\r\n  const formData = reactive({\r\n    city: '', department: '', houseNumber: '', name: '', phone: ''\r\n  })\r\n  const getAddressItem = async () => {\r\n    const result = await get(`/api/user/address/${addressId}`)\r\n    if(result?.errno === 0 && result?.data) {\r\n      const { city, department, houseNumber, name, phone } = result.data;\r\n      formData.city = city\r\n      formData.department = department\r\n      formData.houseNumber = houseNumber\r\n      formData.name = name\r\n      formData.phone = phone\r\n    }\r\n  }\r\n  const upsertAddress = async () => {\r\n    if(addressId) {\r\n      const result = await patch(`/api/user/address/${addressId}`, {\r\n        data: formData\r\n      })\r\n      if (result?.errno === 0) { router.back() } \r\n    }else {\r\n      const result = await post('/api/user/address', {\r\n        data: formData\r\n      })\r\n      if (result?.errno === 0) { router.back() } \r\n    }\r\n  }\r\n  if(addressId) { getAddressItem() }\r\n  return { formData, upsertAddress }\r\n}\r\n\r\nexport default {\r\n  name: 'MyAddressList',\r\n  setup() {\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n    const addressId = route?.params?.id\r\n    const { addressList } = useAddressEffect()\r\n    const { formData, upsertAddress } = useFormEffect(addressId)\r\n    const handleBackClick = () => { router.back() }\r\n    return { addressId, addressList, formData, handleBackClick, upsertAddress }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  background: $dark-bgColor;\r\n}\r\n.title {\r\n  display: flex;\r\n  line-height: .44rem;\r\n  background: $bgColor;\r\n  font-size: .16rem;\r\n  color: $content-fontcolor;\r\n  text-align: center;\r\n  &__back {\r\n    width: .2rem;\r\n    margin-left: .18rem;\r\n    font-size: .2rem;\r\n    color: $search-fontColor;\r\n  }\r\n  &__text {\r\n    flex: 1;\r\n    text-align: center;\r\n  }\r\n  &__save {\r\n    margin-right: .2rem;\r\n    font-size: .14rem;\r\n  }\r\n}\r\n.form {\r\n  padding: 0 .2rem;\r\n  margin-top: .12rem;\r\n  background: $bgColor;\r\n  border-top: .01rem solid $content-bgcolor;\r\n  border-bottom: .01rem solid $content-bgcolor;\r\n  &__item {\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    line-height: .2rem;\r\n    font-size: .14rem;\r\n    border-bottom: .01rem solid $content-bgcolor;\r\n    &:last-of-type {\r\n      border-bottom: none;\r\n    }\r\n    &__label {\r\n      margin-right: .05rem;\r\n      color: $medium-fontColor;\r\n    }\r\n    &__content {\r\n      flex: 1;\r\n      border: none;\r\n      outline: none;\r\n      color: #3F3F3F;\r\n      &::placeholder {\r\n        color: $light-fontColor;\r\n        background-color: red;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}