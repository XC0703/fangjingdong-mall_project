{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport { get } from '../../utils/request';\nimport { useRoute } from 'vue-router'; // 点击获取tab的逻辑\n\nconst useTabEffect = categories => {\n  const data = reactive({\n    currentTab: categories[0].tab\n  });\n\n  const handleTabClick = tab => {\n    data.currentTab = tab;\n  };\n\n  const {\n    currentTab\n  } = toRefs(data);\n  console.log(currentTab);\n  return {\n    currentTab,\n    handleTabClick\n  };\n}; // 根据tab获取对应数据的逻辑\n\n\nconst getContentEffect = () => {\n  const route = useRoute();\n  const shopId = route.params.id;\n  const data = reactive({\n    contentList: []\n  });\n\n  const getContentData = async tab => {\n    const result = await get(`/api/shop/${shopId}/products`, {\n      tab\n    });\n\n    if (result.data.errno === 0) {\n      data.contentList = result.data;\n    }\n  };\n\n  const {\n    contentList\n  } = toRefs(data);\n  return {\n    contentList,\n    getContentData,\n    shopId\n  };\n};\n\nexport default {\n  name: 'ContentView',\n\n  setup() {\n    const categories = [{\n      name: '全部商品',\n      tab: 'all'\n    }, {\n      name: '秒杀',\n      tab: 'seckill'\n    }, {\n      name: '新鲜水果',\n      tab: 'fruit'\n    }];\n    const {\n      currentTab,\n      handleTabClick\n    } = useTabEffect(categories);\n    const {\n      contentList,\n      getContentData,\n      shopId\n    } = getContentEffect();\n    return {\n      categories,\n      currentTab,\n      handleTabClick,\n      contentList,\n      getContentData,\n      shopId\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,SAASA,QAAT,EAAmBC,MAAnB,QAAkC,KAAlC;AACA,SAASC,GAAT,QAAoB,qBAApB;AACA,SAAQC,QAAR,QAAwB,YAAxB,C,CAEA;;AACA,MAAMC,YAAW,GAAKC,UAAD,IAAc;EACjC,MAAMC,IAAG,GAAIN,QAAQ,CAAC;IACpBO,UAAU,EAACF,UAAU,CAAC,CAAD,CAAV,CAAcG;EADL,CAAD,CAArB;;EAGA,MAAMC,cAAa,GAAKD,GAAD,IAAO;IAC9BF,IAAI,CAACC,UAAL,GAAkBC,GAAlB;EACA,CAFA;;EAGA,MAAM;IAAED;EAAF,IAAiBN,MAAM,CAACK,IAAD,CAA7B;EACFI,OAAO,CAACC,GAAR,CAAYJ,UAAZ;EACE,OAAO;IAACA,UAAD;IAAYE;EAAZ,CAAP;AACF,CAVA,C,CAWA;;;AACA,MAAMG,gBAAe,GAAI,MAAM;EAC7B,MAAMC,KAAI,GAAIV,QAAQ,EAAtB;EACA,MAAMW,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;EACA,MAAMV,IAAG,GAAIN,QAAQ,CAAC;IAClBiB,WAAW,EAAC;EADM,CAAD,CAArB;;EAGA,MAAMC,cAAa,GAAI,MAAOV,GAAP,IAAe;IACpC,MAAMW,MAAK,GAAI,MAAMjB,GAAG,CAAE,aAAYY,MAAO,WAArB,EAAiC;MAAEN;IAAF,CAAjC,CAAxB;;IACA,IAAIW,MAAM,CAACb,IAAP,CAAYc,KAAZ,KAAsB,CAA1B,EAA6B;MAC3Bd,IAAI,CAACW,WAAL,GAAmBE,MAAM,CAACb,IAA1B;IACF;EACF,CALA;;EAMA,MAAM;IAAEW;EAAF,IAAkBhB,MAAM,CAACK,IAAD,CAA9B;EACA,OAAO;IAAEW,WAAF;IAAeC,cAAf;IAA+BJ;EAA/B,CAAP;AACF,CAdA;;AAeA,eAAe;EACbO,IAAI,EAAE,aADO;;EAEbC,KAAK,GAAG;IACN,MAAMjB,UAAS,GAAI,CAAC;MAClBgB,IAAI,EAAC,MADa;MAElBb,GAAG,EAAC;IAFc,CAAD,EAGjB;MACAa,IAAI,EAAC,IADL;MAEAb,GAAG,EAAC;IAFJ,CAHiB,EAMjB;MACAa,IAAI,EAAC,MADL;MAEAb,GAAG,EAAC;IAFJ,CANiB,CAAnB;IAWA,MAAM;MAACD,UAAD;MAAYE;IAAZ,IAA8BL,YAAY,CAACC,UAAD,CAAhD;IACA,MAAM;MAAEY,WAAF;MAAeC,cAAf;MAA+BJ;IAA/B,IAA0CF,gBAAgB,EAAhE;IAEA,OAAO;MAACP,UAAD;MAAYE,UAAZ;MAAuBE,cAAvB;MAAsCQ,WAAtC;MAAmDC,cAAnD;MAAmEJ;IAAnE,CAAP;EACF;;AAlBa,CAAf","names":["reactive","toRefs","get","useRoute","useTabEffect","categories","data","currentTab","tab","handleTabClick","console","log","getContentEffect","route","shopId","params","id","contentList","getContentData","result","errno","name","setup"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\shop\\ContentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"category\">\r\n        <div \r\n          class=\"category__item\" \r\n          :class=\"{'category__item--active': currentTab===item.tab}\"\r\n          v-for=\"item in categories\"\r\n          :key=\"item.name\"\r\n          @click=\"()=>handleTabClick(item.tab)\"\r\n        >{{item.name}}</div>\r\n    </div>\r\n    <div class=\"product\">\r\n        <div \r\n          class=\"product__item\"\r\n          v-for=\"item in contentList\"\r\n          :key=\"item._id\"\r\n        >\r\n            <img class=\"product__item__img\" src=\"http://www.dell-lee.com/imgs/vue3/near.png\" />\r\n            <div class=\"product__item__detail\">\r\n              <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n              <p class=\"product__item__sales\">月售 {{item.sales}} 件</p>\r\n              <p class=\"product__item__price\">\r\n                <span class=\"product__item__yen\">&yen;</span>{{item.price}}\r\n                <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n              </p>\r\n            </div>\r\n            <div class=\"product__number\">\r\n              <span class=\"product__number__minus\">-</span>\r\n              0\r\n              <span class=\"product__number__plus\">+</span>\r\n            </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs, } from 'vue';\r\nimport { get } from '../../utils/request';\r\nimport {useRoute } from 'vue-router'\r\n\r\n// 点击获取tab的逻辑\r\nconst useTabEffect = (categories)=>{\r\n  const data = reactive({\r\n    currentTab:categories[0].tab,\r\n  });\r\n  const handleTabClick = (tab)=>{\r\n  data.currentTab = tab;\r\n  }\r\n  const { currentTab } = toRefs(data);\r\nconsole.log(currentTab)\r\n  return {currentTab,handleTabClick}\r\n}\r\n// 根据tab获取对应数据的逻辑\r\nconst getContentEffect = () => {\r\n  const route = useRoute()\r\n  const shopId = route.params.id\r\n  const data = reactive({\r\n      contentList:[]\r\n  });\r\n  const getContentData = async (tab) => {\r\n    const result = await get(`/api/shop/${shopId}/products`, { tab })\r\n    if (result.data.errno === 0) {\r\n      data.contentList = result.data\r\n    }\r\n  }\r\n  const { contentList } = toRefs(data)\r\n  return { contentList, getContentData, shopId }\r\n}\r\nexport default {\r\n  name: 'ContentView',\r\n  setup() {\r\n    const categories = [{\r\n      name:'全部商品',\r\n      tab:'all'\r\n    },{\r\n      name:'秒杀',\r\n      tab:'seckill'\r\n    },{\r\n      name:'新鲜水果',\r\n      tab:'fruit'\r\n    }]\r\n\r\n    const {currentTab,handleTabClick} = useTabEffect(categories);\r\n    const { contentList, getContentData, shopId } = getContentEffect();\r\n\r\n    return {categories,currentTab,handleTabClick,contentList, getContentData, shopId}\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n.content {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 1.5rem;\r\n  bottom: .5rem;\r\n}\r\n.category {\r\n  overflow-y: scroll;\r\n  height: 100%;\r\n  width: .76rem;\r\n  background: $search-bgColor;\r\n  &__item {\r\n    line-height: .4rem;\r\n    text-align: center;\r\n    font-size: 14px;\r\n    color: $content-fontcolor;\r\n    &--active {\r\n      background: $bgColor;\r\n    }\r\n  }\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    margin: 0 .16rem;\r\n    border-bottom: .01rem solid $content-bgcolor;\r\n    &__detail{\r\n      overflow: hidden;\r\n    }\r\n    &__img {\r\n      width: .68rem;\r\n      height: .68rem;\r\n      margin-right: .16rem;\r\n    }\r\n    &__title{\r\n      margin:0;\r\n      line-height:.2rem;\r\n      font-size: .14rem;\r\n      color:$content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__sales{\r\n      margin:.06rem 0;\r\n      line-height: .16rem;\r\n      font-size: .12rem;\r\n      color:$content-fontcolor;\r\n    }\r\n    &__price{\r\n      margin:0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color:$hightlight-fontColor ;\r\n    }\r\n    &__yen{\r\n      font-size: .12rem;\r\n    }\r\n    &__origin{\r\n      margin-left: .06rem;\r\n      line-height: .2rem;\r\n      font-size: .12rem;\r\n      color:$light-fontColor;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number{\r\n      position: absolute;\r\n      right:0;\r\n      bottom:.12rem;\r\n      &__minus,&__plus{\r\n        display: inline-block;\r\n        width: .2rem;\r\n        height: .2rem;\r\n        line-height: .16rem;\r\n        border-radius: 50%;\r\n        font-size: .2rem;     \r\n        text-align: center;\r\n      }\r\n      &__minus{\r\n        border: .01rem solid $medium-fontColor;\r\n        color: $medium-fontColor;\r\n        margin-right: .05rem;\r\n      }\r\n      &__plus{\r\n        margin-left: .05rem;\r\n        background-color: #0091ff;\r\n        color: $bgColor;\r\n      }\r\n    }\r\n  }\r\n  \r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}