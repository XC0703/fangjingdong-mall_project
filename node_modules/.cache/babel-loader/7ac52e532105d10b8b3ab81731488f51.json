{"ast":null,"code":"import FooTer from '../../components/FooTer';\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\n\nconst useCartEffect = () => {\n  const store = useStore();\n  const cartList = store.state.cartList;\n  const cartListWithProducts = computed(() => {\n    let total = 0;\n    const newCartList = {}; // 遍历所有商铺\n\n    for (let shopId in cartList) {\n      const productList = cartList[shopId]?.productList || {}; // 遍历某个商铺的所有商铺，其总数量不能为0\n\n      for (let i in productList) {\n        const product = productList[i];\n        total += product.count || 0;\n      }\n\n      if (total > 0) {\n        newCartList[shopId] = cartList[shopId];\n      }\n    }\n\n    return newCartList;\n  });\n  return {\n    cartListWithProducts\n  };\n};\n\nexport default {\n  name: 'CartList',\n  components: {\n    FooTer\n  },\n\n  setup() {\n    const {\n      cartListWithProducts\n    } = useCartEffect();\n    console.log(cartListWithProducts);\n    return {\n      cartListWithProducts\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;;AAEA,MAAMC,aAAY,GAAI,MAAM;EACxB,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;EACA,MAAMG,QAAO,GAAID,KAAK,CAACE,KAAN,CAAYD,QAA7B;EAEA,MAAME,oBAAmB,GAAIN,QAAQ,CAAC,MAAM;IAC1C,IAAIO,KAAI,GAAI,CAAZ;IACA,MAAMC,WAAU,GAAI,EAApB,CAF0C,CAG1C;;IACA,KAAI,IAAIC,MAAR,IAAkBL,QAAlB,EAA4B;MACxB,MAAMM,WAAU,GAAIN,QAAQ,CAACK,MAAD,CAAR,EAAkBC,WAAlB,IAAiC,EAArD,CADwB,CAExB;;MACA,KAAI,IAAIC,CAAR,IAAaD,WAAb,EAA0B;QACxB,MAAME,OAAM,GAAIF,WAAW,CAACC,CAAD,CAA3B;QACAJ,KAAI,IAAMK,OAAO,CAACC,KAAR,IAAiB,CAA3B;MACF;;MACA,IAAGN,KAAI,GAAI,CAAX,EAAc;QACZC,WAAW,CAACC,MAAD,CAAX,GAAsBL,QAAQ,CAACK,MAAD,CAA9B;MACF;IACJ;;IACA,OAAOD,WAAP;EACD,CAhBoC,CAArC;EAkBA,OAAO;IAAEF;EAAF,CAAP;AACJ,CAvBA;;AAyBA,eAAe;EACXQ,IAAI,EAAC,UADM;EAEXC,UAAU,EAAC;IAAChB;EAAD,CAFA;;EAGXiB,KAAK,GAAE;IACH,MAAM;MAAEV;IAAF,IAA4BJ,aAAa,EAA/C;IACAe,OAAO,CAACC,GAAR,CAAYZ,oBAAZ;IACA,OAAO;MAAEA;IAAF,CAAP;EACJ;;AAPW,CAAf","names":["FooTer","computed","useStore","useCartEffect","store","cartList","state","cartListWithProducts","total","newCartList","shopId","productList","i","product","count","name","components","setup","console","log"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\cartList\\CartList.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n      <div class=\"title\">我的购物车</div>\r\n      <div \r\n        class=\"shops\"\r\n        v-for=\"(shop, index) in cartListWithProducts\"\r\n        :key=\"index\"\r\n      >\r\n      <div class=\"products__title\">{{shop.shopName}}</div>\r\n        <div class=\"products\">\r\n            \r\n            <div class=\"products__wrapper\">\r\n                <div class=\"products__list\">\r\n                    <!-- 在商品数量为0时无需展示，因此需要用到v-if -->\r\n                    <!-- 但v-if和v-for不能同时使用，因此需要在外面包裹一个template -->\r\n                    <template                   \r\n                        v-for=\"item in shop.productList\"\r\n                        :key=\"item._id\"\r\n                    >\r\n                        <div class=\"products__item\" v-if=\"item.count>0\">\r\n                            <img class=\"products__item__img\" :src=\"item.imgUrl\" />\r\n                            <div class=\"products__item__detail\">\r\n                                <h4 class=\"products__item__title\">{{item.name}}</h4>\r\n                                <p class=\"products__item__price\">\r\n                                    <span class=\"products__item__price__single\">\r\n                                        <span class=\"products__item__yen\">&yen;</span>\r\n                                        {{item.price}} × {{item.count}}\r\n                                    </span>\r\n                                    <span class=\"products__item__price__total\">\r\n                                        <span class=\"products__item__yen\">&yen;</span>\r\n                                        {{(item.price*item.count).toFixed(2)}}\r\n                                    </span>\r\n                                </p>\r\n                            </div>\r\n                        </div>\r\n                    </template>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n  <FooTer :currentIndex=\"1\" />\r\n</template>\r\n\r\n<script>\r\nimport FooTer from '../../components/FooTer';\r\nimport { computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\n\r\nconst useCartEffect = () => {\r\n    const store = useStore();\r\n    const cartList = store.state.cartList;\r\n\r\n    const cartListWithProducts = computed(() => {\r\n      let total = 0;\r\n      const newCartList = {};\r\n      // 遍历所有商铺\r\n      for(let shopId in cartList) {\r\n          const productList = cartList[shopId]?.productList || {}\r\n          // 遍历某个商铺的所有商铺，其总数量不能为0\r\n          for(let i in productList) {\r\n            const product = productList[i];\r\n            total += (product.count || 0);\r\n          }\r\n          if(total > 0) {\r\n            newCartList[shopId] = cartList[shopId];\r\n          }\r\n      }\r\n      return newCartList\r\n    })\r\n\r\n    return { cartListWithProducts }\r\n}\r\n\r\nexport default {\r\n    name:'CartList',\r\n    components:{FooTer},\r\n    setup(){\r\n        const { cartListWithProducts } =  useCartEffect()  \r\n        console.log(cartListWithProducts)\r\n        return { cartListWithProducts }      \r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n.wrapper {\r\n    // 作用是使纵向出现滚动条\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  bottom: .5rem;\r\n  right: 0;\r\n  padding: 0 .18rem .1rem .18rem;\r\n}\r\n.title {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  z-index: 1;\r\n  line-height: .44rem;\r\n  background: $bgColor;\r\n  font-size: .16rem;\r\n  color: $content-fontcolor;\r\n  text-align: center;\r\n}\r\n.shops {\r\n  overflow-y: scroll;\r\n  position: absolute;\r\n  top: .6rem;\r\n  right: .18rem;\r\n  bottom: .1rem;\r\n  left: .18rem;\r\n  background: $bgColor;\r\n}\r\n.products{\r\n    margin: .16rem .18rem .1rem .18rem;\r\n    background-color: $bgColor;\r\n    &__wrapper{\r\n        overflow-y: scroll;\r\n        position: absolute;\r\n        left: 0;\r\n        right: 0;\r\n        top:2.6rem;\r\n        bottom: .6rem;\r\n        margin:0 .18rem;\r\n    }\r\n    &__list{\r\n        background-color: $bgColor;\r\n    }\r\n    &__title{\r\n        line-height: .22rem;\r\n        padding:.16rem;\r\n        color:#333;\r\n        font-size: .16rem;\r\n    }\r\n    &__item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: .16rem .16rem .16rem;\r\n        &__img {\r\n            width: .46rem;\r\n            height: .46rem;\r\n            margin-right: .16rem;\r\n        }\r\n        &__detail {\r\n            overflow: hidden;\r\n            flex:1;\r\n        }\r\n        &__title {\r\n            margin: 0;\r\n            line-height: .2rem;\r\n            font-size: .14rem;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &__price {\r\n            display: flex;\r\n            margin: .06rem 0 0 0;\r\n            line-height: .2rem;\r\n            font-size: .14rem;\r\n            color: $hightlight-fontColor;\r\n            &__total{\r\n                flex:1;\r\n                text-align: right;\r\n                color:$dark-fontColor;\r\n            }\r\n        }\r\n        &__yen {\r\n            font-size: .12rem;\r\n        }\r\n    }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}