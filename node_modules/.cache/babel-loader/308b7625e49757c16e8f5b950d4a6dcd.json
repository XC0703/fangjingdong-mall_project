{"ast":null,"code":"import { reactive } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { get } from '../../utils/request';\nimport ShopInfo from '../../components/ShopInfo';\n\nconst useShopInsoEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n\n  const getItemData = async () => {\n    const result = await get(`api/shop/${route.params.id}`);\n\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data;\n    }\n  };\n\n  return {\n    data,\n    getItemData\n  };\n};\n\nexport default {\n  name: 'ShopView',\n  components: {\n    ShopInfo\n  },\n\n  setup() {\n    const router = useRouter();\n    const {\n      data,\n      getItemData\n    } = useShopInsoEffect();\n    getItemData();\n\n    const handleBackClick = () => {\n      router.back();\n    };\n\n    return {\n      data,\n      handleBackClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,SAAT,EAAmBC,QAAnB,QAAmC,YAAnC;AACA,SAASC,GAAT,QAAoB,qBAApB;AACA,OAAOC,QAAP,MAAqB,2BAArB;;AAEA,MAAMC,iBAAgB,GAAI,MAAI;EAC1B,MAAMC,KAAI,GAAIJ,QAAQ,EAAtB;EACA,MAAMK,IAAG,GAAIP,QAAQ,CAAC;IACtBQ,IAAI,EAAC;EADiB,CAAD,CAArB;;EAGA,MAAMC,WAAU,GAAI,YAAS;IACzB,MAAMC,MAAK,GAAI,MAAMP,GAAG,CAAE,YAAWG,KAAK,CAACK,MAAN,CAAaC,EAAG,EAA7B,CAAxB;;IACA,IAAGF,MAAM,EAAEG,KAAR,KAAkB,CAAlB,IAAqBH,MAAM,EAAEH,IAAhC,EAAqC;MACjCA,IAAI,CAACC,IAAL,GAAYE,MAAM,CAACH,IAAnB;IACJ;EACJ,CALA;;EAMA,OAAO;IAAEA,IAAF;IAAOE;EAAP,CAAP;AACJ,CAZA;;AAcA,eAAe;EACXK,IAAI,EAAC,UADM;EAEXC,UAAU,EAAC;IAACX;EAAD,CAFA;;EAGXY,KAAK,GAAE;IACH,MAAMC,MAAK,GAAIhB,SAAS,EAAxB;IAEA,MAAM;MAACM,IAAD;MAAME;IAAN,IAAqBJ,iBAAiB,EAA5C;IAEAI,WAAW;;IACX,MAAMS,eAAc,GAAI,MAAI;MACxBD,MAAM,CAACE,IAAP;IACJ,CAFA;;IAGA,OAAO;MAACZ,IAAD;MAAOW;IAAP,CAAP;EACJ;;AAbW,CAAf","names":["reactive","useRouter","useRoute","get","ShopInfo","useShopInsoEffect","route","data","item","getItemData","result","params","id","errno","name","components","setup","router","handleBackClick","back"