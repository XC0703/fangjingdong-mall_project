{"ast":null,"code":"import AddRess from '../../components/AddRess';\nimport useCommonAddressEffect from '../../effects/addressEffect';\nimport { toRefs } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: 'AddressList',\n  components: {\n    AddRess\n  },\n\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const {\n      addressList\n    } = toRefs(store.state); // 发送请求获取地址数据\n\n    const {\n      getAddressList\n    } = useCommonAddressEffect();\n    getAddressList(true);\n\n    const handleBackClick = () => {\n      router.back();\n    };\n\n    const handleAddClick = () => {\n      router.push({\n        name: 'AddressEdit'\n      });\n    };\n\n    const handleUpdateClick = addressId => {\n      router.push(`/addressEdit/${addressId}`);\n    };\n\n    return {\n      addressList,\n      handleBackClick,\n      handleAddClick,\n      handleUpdateClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,sBAAP,MAAmC,6BAAnC;AAEA,SAASC,MAAT,QAAuB,KAAvB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,QAA0B,YAA1B;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IAAEN;EAAF,CAFC;;EAGbO,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,EAAtB;IACA,MAAMM,MAAK,GAAIL,SAAS,EAAxB;IACA,MAAM;MAAEM;IAAF,IAAkBR,MAAM,CAACM,KAAK,CAACG,KAAP,CAA9B,CAHM,CAIN;;IACA,MAAM;MAAEC;IAAF,IAAqBX,sBAAsB,EAAjD;IACAW,cAAc,CAAC,IAAD,CAAd;;IACA,MAAMC,eAAc,GAAI,MAAM;MAAEJ,MAAM,CAACK,IAAP;IAAc,CAA9C;;IACA,MAAMC,cAAa,GAAI,MAAM;MAAEN,MAAM,CAACO,IAAP,CAAY;QAAEX,IAAI,EAAE;MAAR,CAAZ;IAAoC,CAAnE;;IACA,MAAMY,iBAAgB,GAAKC,SAAD,IAAc;MAACT,MAAM,CAACO,IAAP,CAAa,gBAAeE,SAAU,EAAtC;IAAwC,CAAjF;;IACA,OAAO;MAAER,WAAF;MAAeG,eAAf;MAAgCE,cAAhC;MAAgDE;IAAhD,CAAP;EACF;;AAda,CAAf","names":["AddRess","useCommonAddressEffect","toRefs","useStore","useRouter","name","components","setup","store","router","addressList","state","getAddressList","handleBackClick","back","handleAddClick","push","handleUpdateClick","add