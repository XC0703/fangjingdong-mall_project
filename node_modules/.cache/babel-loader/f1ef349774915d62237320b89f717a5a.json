{"ast":null,"code":"import AddRess from '../../components/AddRess';\nimport useCommonAddressEffect from '../../effects/addressEffects';\nimport { toRefs } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  name: 'AddressChoose',\n  components: {\n    AddRess\n  },\n\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      addressList\n    } = toRefs(store.state);\n    const {\n      getAddressList\n    } = useCommonAddressEffect();\n    getAddressList();\n\n    const handleBackClick = () => {\n      router.back();\n    };\n\n    const handleAddressClick = id => {\n      router.push(`/orderConfirm/${shopId}/${id}`);\n    };\n\n    return {\n      addressList,\n      handleBackClick,\n      handleAddressClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,sBAAP,MAAmC,8BAAnC;AAEA,SAASC,MAAT,QAAuB,KAAvB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AAEA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,UAAU,EAAE;IAAEP;EAAF,CAFC;;EAGbQ,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,EAAtB;IACA,MAAMO,MAAK,GAAIN,SAAS,EAAxB;IACA,MAAMO,KAAI,GAAIN,QAAQ,EAAtB;IACA,MAAMO,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;IACA,MAAM;MAAEC;IAAF,IAAkBb,MAAM,CAACO,KAAK,CAACO,KAAP,CAA9B;IACA,MAAM;MAAEC;IAAF,IAAqBhB,sBAAsB,EAAjD;IACAgB,cAAc;;IACd,MAAMC,eAAc,GAAI,MAAM;MAAER,MAAM,CAACS,IAAP;IAAc,CAA9C;;IACA,MAAMC,kBAAiB,GAAKN,EAAD,IAAQ;MACjCJ,MAAM,CAACW,IAAP,CAAa,iBAAgBT,MAAO,IAAGE,EAAG,EAA1C;IACF,CAFA;;IAGA,OAAO;MAAEC,WAAF;MAAeG,eAAf;MAAgCE;IAAhC,CAAP;EACF;;AAhBa,CAAf","names":["AddRess","useCommonAddressEffect","toRefs","useStore","useRouter","useRoute","name","components","setup","store","router","route","shopId","params","id","addressList","state","getAddressList","handleBackClick","back","handleAddressClick","push"],"sourceRoot":"","sources":["D:\\StudySoft\\VSCode\\VSCodeFile\\CODE_前端projects\\fangjingdong-mall_project\\src\\views\\addressChoose\\AddressChoose.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"title\">\r\n      <div\r\n        class=\"iconfont title__icon\"\r\n        @click=\"handleBackClick\"\r\n      >&#xe6f2;</div>\r\n      收货地址\r\n    </div>\r\n    <AddRess\r\n      v-for=\"address in addressList\"\r\n      :key=\"address._id\"\r\n      :address=\"address\"\r\n      @click=\"() => handleAddressClick(address._id)\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AddRess from '../../components/AddRess'\r\nimport useCommonAddressEffect from '../../effects/addressEffects'\r\n\r\nimport { toRefs } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRouter, useRoute } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'AddressChoose',\r\n  components: { AddRess },\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { addressList } = toRefs(store.state)\r\n    const { getAddressList } = useCommonAddressEffect()\r\n    getAddressList()\r\n    const handleBackClick = () => { router.back() }\r\n    const handleAddressClick = (id) => {\r\n      router.push(`/orderConfirm/${shopId}/${id}`)\r\n    }\r\n    return { addressList, handleBackClick, handleAddressClick }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  background: $dark-bgColor;\r\n}\r\n.title {\r\n  position: relative;\r\n  line-height: .44rem;\r\n  background: $bgColor;\r\n  font-size: .16rem;\r\n  color: $content-fontcolor;\r\n  text-align: center;\r\n  &__icon {\r\n    position: absolute;\r\n    left: .18rem;\r\n    top: 0;\r\n    font-size: .2rem;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}